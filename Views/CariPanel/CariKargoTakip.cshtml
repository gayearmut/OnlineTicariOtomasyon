@using OnlineTicariOtomasyon.Models.Siniflar
@model List<KargoTakip>
@{
	ViewBag.Title = "CariKargoTakip";
	Layout = "~/Views/Shared/_CariLayout.cshtml";
}

<h2>Cari Kargo Takip</h2>

@{int sayac = 0; }
<br />
<table class="table table-striped">
	<tr>
		<th>#</th>
		<th>Açıklama</th>
		<th>Tarih</th>
	</tr>
	@foreach (var x in Model)
	{
		<tr>
			<td>@{sayac++;} @sayac</td>
			<td>@x.Aciklama</td>
			<td>@x.TarihZaman</td>
		</tr>
	}
</table>