@model OnlineTicariOtomasyon.Models.Siniflar.Departman
@{
	ViewBag.Title = "DepartmanEkle";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Departman Ekleme Sayfası</h2>
<br />

<form class=" form-group" method="post">

	@Html.LabelFor(x => x.DepartmanAd)

	@Html.TextBoxFor(x => x.DepartmanAd, new { @class = "form-control" })
	<br />
	<button class="btn btn-info"><i class="fa fa-plus"></i> Kaydet</button>
</form>



